{
  "name": "slack-archive-browser",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "scripts/start_local_db.sh && next dev && scripts/stop_local_db.sh",
    "build": "next build",
    "build:vercel": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint && tsc --noemit",
    "db:local:start": "scripts/start_local_db.sh",
    "db:local:stop": "scripts/stop_local_db.sh",
    "db:reset": "prisma migrate reset",
    "db:migrate:dev": "prisma migrate dev",
    "archive:import": "tsx scripts/archive_to_db.ts .archive",
    "archive:import:test": "tsx scripts/archive_to_db.ts .archive export-test"
  },
  "dependencies": {
    "@prisma/client": "6.2.1",
    "clsx": "2.1.1",
    "emoji-js": "3.8.1",
    "next": "14.2.23",
    "next-auth": "4.24.11",
    "react": "18.3.0",
    "react-dom": "18.3.0",
    "react-toastify": "11.0.3",
    "server-only": "0.0.1",
    "use-debounce": "10.0.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.0.0",
    "@types/emoji-js": "3.5.2",
    "@types/node": "22.10.10",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/parser": "8.21.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.23",
    "eslint-config-prettier": "10.0.1",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.11",
    "prisma": "6.2.1",
    "tailwindcss": "4.0.0",
    "tsx": "4.19.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^22",
    "pnpm": "^10.11"
  }
}
